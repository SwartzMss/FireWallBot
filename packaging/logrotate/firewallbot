# Logrotate configuration for FireWallBot logs
# Rotate when log file exceeds 20 MiB. Keep up to 10 compressed archives and
# remove the oldest when exceeding that number.

/home/swartz/WorkSpace/FireWallBot/log/*.jsonl {
    size 20M
    rotate 10
    compress
    delaycompress
    copytruncate
    missingok
    notifempty
    create 0644 root root
}
